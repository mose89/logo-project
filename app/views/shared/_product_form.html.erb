<%= form_for @product do |f| %>

  <%= f.label :produkt_navn %> <br>
  <%= f.text_field :name %> <br>
  <%= f.label :beskrivelse %> <br>
  <%= f.text_area :description %> <br>

  <%= f.label :pris %> <br>
  <%= f.number_field :price %>

  <%= f.label :foto %> <br>
  <%= f.file_field :photo %>
  <%= f.hidden_field :photo_cache %> <br><br>

  <%= f.submit "Lagre Produkt", class: "btn btn-lg btn-success" %>
<% end %>
