<%= form_for @gallery do |f| %>

  <%= f.label :tittel %> <br>
  <%= f.text_field :header %> <br>

  <%= f.label :tekst %> <br>
  <%= f.text_area :description %> <br>

  <%= f.label :foto %> <br>
  <%= f.file_field :photo %>
  <%= f.hidden_field :photo_cache %> <br><br>

  <%= f.submit "Lagre Galleri Bilde", class: "btn btn-lg btn-success" %>
<% end %>
